{
	"name": "wikiparser-node",
	"version": "0.11.0",
	"description": "A Node.js parser for MediaWiki markup with AST",
	"keywords": [
		"mediawiki",
		"wikitext",
		"parser"
	],
	"homepage": "https://github.com/bhsd-harry/wikiparser-node/wiki",
	"bugs": {
		"url": "https://github.com/bhsd-harry/wikiparser-node/issues"
	},
	"license": "GPL-3.0",
	"author": "Bhsd",
	"files": [
		"/main.js",
		"/errors/README",
		"/printed/README",
		"/config/",
		"/i18n/",
		"/dist/"
	],
	"main": "/dist/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bhsd-harry/wikiparser-node.git"
	},
	"scripts": {
		"prepublishOnly": "rm -rf dist/; tsc; rm dist/[mpu]*/*.d.ts && grep -rl --include='*.d.ts' '@private' dist/ | xargs gsed -i '/@private/,+1d'",
		"build": "rm -rf dist/; tsc; eslint --fix dist/",
		"diff": "./bin/diff.sh"
	},
	"devDependencies": {
		"@types/node": "^17.0.23",
		"eslint": "^8.30.0",
		"eslint-plugin-n": "^15.6.0",
		"eslint-plugin-regexp": "^1.11.0",
		"eslint-plugin-unicorn": "^45.0.2",
		"eslint-plugin-jsdoc": "^1.0.0",
		"eslint-plugin-json-es": "^1.5.7",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"typescript": "^4.6.3",
		"@typescript-eslint/eslint-plugin": "^5.47.1",
		"@typescript-eslint/parser": "^5.47.1",
		"ajv-cli": "^5.0.0",
		"request": "^2.88.2"
	},
	"engines": {
		"node": "^18.4.0"
	}
}
